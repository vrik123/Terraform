#!groovy
import groovy.txt.*
import groovy.txt.markup.*
node{
	environment {
    	tfHome = tool name: 'Terraorm-12', type: 'terraform'
 	 }
	stage('Test'){
		deleteDir()
		dir('Terraform'){
			git branch: 'main', credentialsId: 'git-creds', url: 'https://github.com/vrik123/Terraform.git'
			echo "WORKSPACE ${env.WORKSPACE}"
			sh "${tfHome}/bin/terraform init"
  	
		}
	 }

}
